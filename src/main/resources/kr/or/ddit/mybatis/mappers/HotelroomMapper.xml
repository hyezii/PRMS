<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="kr.or.ddit.franchise.franchisee.mapper.HotelroomMapper">
	<select id="selectHotelType" resultType="CmmncodeDefaultVO">
	    SELECT CMMN_CODE_NM, CMMN_REMARK1, CMMN_REMARK2, CMMN_REMARK3
		FROM CMMNCODE 
		WHERE CMMNCL_CODE_ID='COMN14'
  </select>
</mapper>